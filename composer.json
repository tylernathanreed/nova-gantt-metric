{
    "name": "reedware/nova-gantt-metric",
    "description": "Adds Gantt metrics to your Nova application.",
    "keywords": [
        "laravel",
        "nova",
        "gantt",
        "metric"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.3.0"
    },
    "autoload": {
        "psr-4": {
            "Reedware\\NovaGanttMetric\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Reedware\\NovaGanttMetric\\GanttServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
